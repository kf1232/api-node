    "jsonwebtoken": "^9.0.2",
    "sequelize": "^6.6.2",
    "sqlite3": "^5.0.0",
    "swagger-autogen": "^2.23.0",
    "swagger-ui-express": "^5.0.0",
    "tedious": "^18.2.1",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "concurrently": "^6.2.1",
